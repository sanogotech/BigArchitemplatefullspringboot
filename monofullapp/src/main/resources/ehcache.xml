<ehcache>
    <diskStore path="java.io.tmpdir" />
 
    <defaultCache maxElementsInMemory="2000"
            eternal="true"
            overflowToDisk="false"
            timeToLiveSeconds="1200" />
             
    <cache name="employeeCache"
            maxElementsInMemory="2000"
            eternal="false"
            overflowToDisk="false"
            timeToLiveSeconds="10000" />
</ehcache>